/*所有页面*/
.main{margin-left: -15px;margin-right: -15px;}
.page-header{padding-bottom:20px;margin:0;}
.btn-group-sm > .btn, .btn-sm{height: 34px;}
table{font-size: 13px;white-space:nowrap;word-break: keep-all;}
/*IE8兼容*/
.col-md-2{width: 16.6667%\9;float: left\9;}
.col-md-10{width: 83.3333%\9;float: left\9;}
.page-header{float: left\9;width: 100%\9;}
.manager_query_group{width: 320px\9;float: left\9;height: 34px\9;margin-top: 0px\9;margin-bottom: 0px\9;}
.manager_query_group label{float: left\9;line-height: 34px\9;}
.manager_query_group input{padding-top: 8px\9;}
.input-group{float: left\9;width: 175px\9;}
.form-control{width: 175px\9;float: left\9;}

/*有表格的页面*/
.sticky-wrap {overflow-x: visible;overflow-y: auto;position: relative;width: 100%;height: auto;float:left;}
.sticky-wrap .sticky-thead,.sticky-wrap .sticky-col,.sticky-wrap .sticky-intersect {opacity: 0;opacity: 0\9;position: absolute;top: 0;left: 0;transition: all .125s ease-in-out;width: auto;}
.table_align_all th{background-color: #f5f5f5;color: #777;font-weight: bold;white-space: nowrap;border:1px solid #ddd;border-bottom:1px solid #ddd;text-align: center;padding: 0 8px;height: 40px;line-height: 40px;}
/*首页*/
.navbar-default{width: 100%;width: 100%\9;background-image: url(../image/top_pic.jpg);background-repeat: no-repeat;margin-bottom: 0px;background-position: center center;-webkit-background-size: cover;-moz-background-size: cover;-o-background-size: cover;background-size: cover;filter: progid:DXImageTransform.Microsoft.AlphaImageLoader(src='image/top_pic.jpg',sizingMethod='scale');}
.navbar-brand small{color: #fff;font-size: 20px;}
.index_btn_out{margin-top: 8px;margin-left:32px\9;}
.index_nav{position: relative;width: 100%;width: 100%\9;}
.index_nav_left{position: absolute;left: 15px;}
.index_nav_right{position: absolute;right: 30px;right: 30px\9;width: 280px;width: 280px\9;}
.table_align_all>thead>tr>th,.table_align_all>tbody>tr>td{text-align: center;vertical-align: middle;}
.table_align_all tr th{background-color: #f5f5f5;}
.table_align_color{color: #2b7dbc;}
/*法定节假日*/
.row_content{height: 200px;}
.row_content p{line-height: 200px;}
.row_content p>span{margin-left: 120px;}
.order_plan_btn2{margin-top: 82px;}
/*法定节假日导入文件*/
.upload_img{background-color: #2b7dbc;border-radius: 4px;color: #fff;cursor: pointer;display: inline-block;font-size: 14px;line-height: 1.42857;margin-bottom: 0; padding: 7px 13px;text-decoration: none;vertical-align: middle;text-align: center;margin-top: 84px;margin-right: 20px;}
.upload_img:hover{text-decoration: none;color: #fff;}
.upload_img label{ display: block;height: 100%;width: 100%;} 
.upload_file{background: none repeat scroll 0 0 #333;height: 40px;margin: 80px -94px 0 auto;filter:alpha(opacity=0);-moz-opacity:0;-khtml-opacity: 0;opacity: 0;position: absolute;top: 0;width: 58px;z-index: 2;}
/*排班计划导入*/
.order_plan_header{float: left;width: 100%;}
.order_plan_btn1{background-color: #4e5a6d;border: none;color: #fff;}
.order_plan_btn1.active,.order_plan_btn1.focus,.order_plan_btn1:active,.order_plan_btn1:focus,.order_plan_btn1:hover{background-color: #4e5a6d;border: none;}
.upload_img_plan{float:left;background-color: #e79247;border-radius: 3px;color: #fff;cursor: pointer;display: inline-block;font-size: 12px;height: 34px;line-height: 24px;text-decoration: none;text-align: center;padding: 5px 10px;margin-left: 10px;}
.upload_img_plan:hover{text-decoration: none;color: #fff;}
.upload_img_plan label{display: block;height: 100%;width: 100%;font-weight: normal;} 
.upload_file_plan{height: 34px;margin-top:-32px;margin-left:10px;filter:alpha(opacity=0);-moz-opacity:0;-khtml-opacity: 0;opacity: 0;top: 0;}
.upload_div{position:relative\9;}
.upload_file_plan{position:absolute\9;z-index:3\9;width:92px;left:224px\9;top:32px\9;}
/*排班计划导入的上下班时间段选择*/
.timer_input{text-align: center; width:70px}
#times{text-align: center;}
#times.times{width: 140px;height: 156px; overflow-x: hidden;overflow-x: auto\9;background: #fff;border: 1px solid #ddd;border-top: none;margin: 0;padding: 0;position: absolute;z-index: 3}
#times.times li{list-style: none;background: #fff;display: inline-block;width: 40px;cursor: pointer;margin: 0 10px 0 1px;padding: 3px}
/*每日考勤查询）*/
.manager_query_group{margin-right: 32px;float:left\9;margin-right:0px\9;}
.manager_query_group label,.manager_query_group input{margin-right: 10px;}
.manager_query_h1{text-align: center;font-size: 30px;}
.table_align_all_size>tbody>tr:last-child{background-color: #feffef;}
/*月统计考勤*/
.month_query_line{float: left;float: left\9;width: 100%;margin-bottom: 20px;margin-top:20px;}
.month_query_line p,.month_query_line select{float: left;}
.month_query_line p{margin-top: 8px;margin-right: 5px;font-weight: bold;}
.month_query_line select{margin-right: 32px;}
.select_choose{border:1px solid #ccc;border-radius: 4px;height: 34px;}
/*登录页面*/
.loginpage{background: #fff url(../image/patternbg.png);}
.loginbox{width: 350px; padding: 5px; background: #fff; margin: 7% auto 0 auto; -moz-border-radius: 2px; -webkit-border-radius: 2px;border-radius: 2px; -moz-box-shadow: 0 0 2px rgba(0,0,0,0.3); -webkit-box-shadow: 0 0 2px rgba(0,0,0,0.3); box-shadow: 0 0 2px rgba(0,0,0,0.3);}
.loginboxinner{padding: 20px; background: #32415a url(../image/patternbg.png); -moz-border-radius: 0 2px 2px 0; -webkit-border-radius: 0 0 2px 2px; border-radius: 0 0 2px 2px;}
.logo{font-size: 32px; color: #fff; text-align: center; border-bottom: 1px solid #56647d;line-height: normal; margin-bottom: 5px;color: #FB9337;}
.slogan{color: #666;font-size: 12px;text-align: center;margin-top: -15px;}
.form_side{padding-left: 15px;padding-right: 15px;}
.form_side input{height: 50px;}
.loginbox form{ display: block; margin-top: 60px;}
.loginbox button{background: #f0801d url(../image/btngrad.png) repeat-x top left; border: 0; padding: 15px 0;text-align: center;font-size: 16px;width: 100%;text-transform: uppercase;-moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px; -moz-box-shadow: 0 1px 2px rgba(0,0,0,0.4); -webkit-box-shadow: 0 1px 2px rgba(0,0,0,0.4); box-shadow: 0 1px 2px rgba(0,0,0,0.4);}
.loginbox button:hover{ background-color: #f0721e; }
.form_side_checkbox{margin-left: 20px;}
#inputusername,#inputpwd{width:260px;}
/*登录页面错误信息提示*/
.nousername .loginmsg,.nopassword .loginmsg{display: none;}
.loginmsg{background: #fffccc; color: #333; margin-top: 30px; padding: 5px; text-align: center; font-size: 11px; -moz-border-radius: 2px; -webkit-border-radius: 2px; border-radius: 2px;}