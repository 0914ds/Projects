.tree {min-height:20px;margin-bottom:20px;margin-left: -15px;margin-right: -15px;-webkit-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.05);-moz-box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.05);box-shadow:inset 0 1px 1px rgba(0, 0, 0, 0.05);}
.tree li {list-style-type:none;margin:0;padding:10px 5px 0 20px;position:relative;font-size: 13px;background-color: #f9f9f9;border-top: 1px dotted #e4e4e4;}
.tree li::before, .tree li::after {content:'';left:-20px;position:absolute;right:auto;}
.tree li::before {border-left:1px dotted #9dbdd6;bottom:50px;height:100%;top:0;width:1px;left: 0px;}
.tree li::after {border-top:1px dotted #9dbdd6;height:38px;display: inline-block;position: absolute;top: 25px;width: 7px;left: 0;}
.tree li a {padding: 0 0 0 0;text-decoration:none;display:block;height: 38px;line-height: 36px;text-shadow:none !important;color: #616161;}
.tree li.parent_li>span {cursor:pointer;}
.tree>ul>li::before, .tree>ul>li::after {border:0;}
.tree li:last-child::before {height:30px;}
.parent_li{position: absolute;margin-left: 10px;}
.parent_li_title{height: 38px;line-height: 36px;cursor: pointer;}
.active{color: #2b7dbc;font-weight: bold;}
.active_li{color: #2b7dbc;}
.parent_li_none,.parent_li_has a{margin-left: 10px;}
.parent_li_has{position: relative;margin-left: 10px;}
.s_color{color: #2b7dbc;}
.glyphicon_arrow{position: absolute;right: 30px;top: 20px;cursor: pointer;}